  {q:"‡πÉ‡∏ô‡∏™‡∏∏‡∏Å‡∏£ ‡∏ï‡∏±‡∏ß‡∏≠‡πà‡∏≠‡∏ô‡∏´‡∏•‡∏±‡πà‡∏á estrogen ‡πÄ‡∏û‡∏∑‡πà‡∏≠‡∏¢‡∏±‡∏ö‡∏¢‡∏±‡πâ‡∏á luteolysis ‡πÇ‡∏î‡∏¢‡πÄ‡∏û‡∏¥‡πà‡∏°‡∏Å‡∏≤‡∏£‡∏´‡∏•‡∏±‡πà‡∏á PGF‚ÇÇŒ± ‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà‡∏Å‡∏£‡∏∞‡πÅ‡∏™‡πÄ‡∏•‡∏∑‡∏≠‡∏î", a:false,
   why:"Estrogen ‡πÄ‡∏õ‡∏•‡∏µ‡πà‡∏¢‡∏ô‡∏ó‡∏¥‡∏®‡∏ó‡∏≤‡∏á‡∏Å‡∏≤‡∏£‡∏´‡∏•‡∏±‡πà‡∏á PGF‚ÇÇŒ± ‡∏à‡∏≤‡∏Å endocrine ‚Üí exocrine ‡πÄ‡∏Ç‡πâ‡∏≤‡∏™‡∏π‡πà lumen ‡∏Ç‡∏≠‡∏á‡∏°‡∏î‡∏•‡∏π‡∏Å‡πÅ‡∏ó‡∏ô"}
];

/***********************
 * UI ‚Äì Tabs
 ***********************/
const tabsEl = document.getElementById('tabs');
const views = [
  {id:'mode-mcq', title:'üß© MCQ'},
  {id:'mode-tf',  title:'‚úÖ True / False'}
];
let activeTab = views[0].id;

function renderTabs(){
  tabsEl.innerHTML = '';
  views.forEach(v=>{
    const b = document.createElement('button');
    b.className = 'tab' + (v.id===activeTab ? ' active' : '');
    b.textContent = v.title;
    b.onclick = ()=>{ activeTab = v.id; updateView(); };
    tabsEl.appendChild(b);
  });
}

function updateView(){
  views.forEach(v=>{
    const s = document.getElementById(v.id);
    if(!s) return;
    if(v.id===activeTab){ s.classList.remove('hide'); }
    else{ s.classList.add('hide'); }
  });
  renderTabs();
}

renderTabs();

// ‚úÖ ‡∏≠‡∏±‡∏õ‡πÄ‡∏î‡∏ï‡∏ä‡∏∑‡πà‡∏≠‡πÅ‡∏ó‡πá‡∏ö‡πÉ‡∏´‡πâ‡∏ï‡∏£‡∏á index ‡πÅ‡∏•‡∏∞‡πÑ‡∏°‡πà‡∏≠‡πâ‡∏≤‡∏á‡πÄ‡∏Å‡∏¥‡∏ô
views[0].title = `üß© MCQ (${MCQ.length} ‡∏Ç‡πâ‡∏≠)`;
views[1].title = `‚úÖ True / False (${TF.length} ‡∏Ç‡πâ‡∏≠)`;
renderTabs();
updateView();

/***********************
 * Mode ‚Äì MCQ
 ***********************/
(function(){
  const host=document.getElementById('mode-mcq');
  let order = shuffle(MCQ.map((_,i)=>i));
  let i=0, score=0;
  const total = order.length;

  // ‡∏Å‡∏±‡∏ô‡∏Å‡∏£‡∏ì‡∏µ‡πÑ‡∏°‡πà‡∏°‡∏µ‡∏Ç‡πâ‡∏≠
  if(total===0){
    host.innerHTML = `<div class="qcard">‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÇ‡∏à‡∏ó‡∏¢‡πå MCQ</div>`;
    return;
  }

  const hud=document.createElement('div'); hud.className='scorebox';
  hud.innerHTML = `<div class="badge" id="mcqProg">‡∏Ç‡πâ‡∏≠ 1 / ${total}</div>
                   <div class="badge" id="mcqScore">‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô 0</div>
                   <button class="secondary" id="mcqRestart">‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏´‡∏°‡πà/‡∏™‡∏∏‡πà‡∏°‡πÉ‡∏´‡∏°‡πà</button>`;
  host.appendChild(hud);

  const qc=document.createElement('div'); qc.className='qcard'; host.appendChild(qc);

  function renderQ(){
    const item = MCQ[order[i]];
    document.getElementById('mcqProg').textContent = `‡∏Ç‡πâ‡∏≠ ${i+1} / ${total}`;
    qc.innerHTML = `<div><b>Q${i+1}.</b> ${item.q}</div>`;
    const box=document.createElement('div'); box.className='choices'; qc.appendChild(box);
    const shuffledChoices = shuffle(item.choices.map((c,idx)=>({c,idx})));
    let locked=false;

    shuffledChoices.forEach(({c,idx})=>{
      const div=document.createElement('div');
      div.className='choice'; div.innerHTML=c; box.appendChild(div);
      div.onclick=()=>{
        if(locked) return; locked=true;
        const isCorrect = idx===item.a;
        if(isCorrect){div.classList.add('correct'); score++;}
        else{div.classList.add('wrong');}
        // ‡πÑ‡∏Æ‡πÑ‡∏•‡∏ï‡πå‡∏ï‡∏±‡∏ß‡∏ñ‡∏π‡∏Å
        [...box.children].forEach(el=>{
          const originalIndex=item.choices.indexOf(el.innerHTML);
          if(originalIndex===item.a) el.classList.add('correct');
        });
        // ‡πÄ‡∏â‡∏•‡∏¢
        const exp=document.createElement('div'); exp.className='answer';
        exp.innerHTML = `<b>‡πÄ‡∏â‡∏•‡∏¢:</b> ${item.choices[item.a]}<br><span class="muted">‡πÄ‡∏´‡∏ï‡∏∏‡∏ú‡∏•:</span> ${item.why||'-'}`;
        qc.appendChild(exp);
        // ‡∏õ‡∏∏‡πà‡∏°‡∏ñ‡∏±‡∏î‡πÑ‡∏õ
        const nx=document.createElement('div'); nx.style.marginTop='10px';
        nx.innerHTML = `<button id="btnNext">‡∏ñ‡∏±‡∏î‡πÑ‡∏õ</button>`; qc.appendChild(nx);
        document.getElementById('mcqScore').textContent = `‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô ${score}`;
        document.getElementById('btnNext').onclick = ()=>{
          i++; if(i>=total){
            qc.innerHTML = `<div class="row"><div class="badge">‡∏à‡∏ö‡∏ä‡∏∏‡∏î MCQ</div><div class="badge">‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô‡∏£‡∏ß‡∏°: ${score} / ${total}</div></div>`;
          } else { renderQ(); }
        };
      };
    });
  }

  document.getElementById('mcqRestart').onclick=()=>{
    order = shuffle(MCQ.map((_,i)=>i)); i=0; score=0;
    document.getElementById('mcqScore').textContent='‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô 0';
    renderQ();
  };

  renderQ();
})();

/***********************
 * Mode ‚Äì True / False
 ***********************/
(function(){
  const host=document.getElementById('mode-tf');
  let order = shuffle(TF.map((_,i)=>i));
  let i=0, score=0;
  const total = order.length;

  if(total===0){
    host.innerHTML = `<div class="qcard">‡∏¢‡∏±‡∏á‡πÑ‡∏°‡πà‡∏°‡∏µ‡πÇ‡∏à‡∏ó‡∏¢‡πå True/False</div>`;
    return;
  }

  const hud=document.createElement('div'); hud.className='scorebox';
  hud.innerHTML = `<div class="badge" id="tfProg">‡∏Ç‡πâ‡∏≠ 1 / ${total}</div>
                   <div class="badge" id="tfScore">‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô 0</div>
                   <button class="secondary" id="tfRestart">‡πÄ‡∏£‡∏¥‡πà‡∏°‡πÉ‡∏´‡∏°‡πà/‡∏™‡∏∏‡πà‡∏°‡πÉ‡∏´‡∏°‡πà</button>`;
  host.appendChild(hud);

  const qc=document.createElement('div'); qc.className='qcard'; host.appendChild(qc);

  function renderQ(){
    const item=TF[order[i]];
    document.getElementById('tfProg').textContent=`‡∏Ç‡πâ‡∏≠ ${i+1} / ${total}`;
    qc.innerHTML = `<div><b>Q${i+1}.</b> ${item.q}</div>`;

    const btnRow=document.createElement('div'); btnRow.style.display='flex'; btnRow.style.gap='8px';
    const btnT=document.createElement('button'); btnT.textContent='‡∏ñ‡∏π‡∏Å (True)';
    const btnF=document.createElement('button'); btnF.textContent='‡∏ú‡∏¥‡∏î (False)'; btnF.className='secondary';
    btnRow.append(btnT,btnF); qc.appendChild(btnRow);

    let locked=false;
    function mark(ans){
      if(locked) return; locked=true;
      const cor = item.a===ans; if(cor) score++;
      document.getElementById('tfScore').textContent=`‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô ${score}`;
      const exp=document.createElement('div'); exp.className='answer';
      exp.innerHTML=`<b>‡πÄ‡∏â‡∏•‡∏¢:</b> ${item.a?'<span class="ok">True</span>':'<span class="ng">False</span>'} ¬∑ <span class="muted">‡πÄ‡∏´‡∏ï‡∏∏‡∏ú‡∏•:</span> ${item.why||'-'}`;
      qc.appendChild(exp);
      const nx=document.createElement('div'); nx.style.marginTop='10px'; nx.innerHTML='<button id="tfNext">‡∏ñ‡∏±‡∏î‡πÑ‡∏õ</button>'; qc.appendChild(nx);
      document.getElementById('tfNext').onclick=()=>{
        i++; if(i>=total){
          qc.innerHTML = `<div class="row"><div class="badge">‡∏à‡∏ö‡∏ä‡∏∏‡∏î True/False</div><div class="badge">‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô‡∏£‡∏ß‡∏°: ${score} / ${total}</div></div>`;
        } else { renderQ(); }
      };
    }
    btnT.onclick=()=>mark(true); btnF.onclick=()=>mark(false);
  }

  document.getElementById('tfRestart').onclick=()=>{
    order = shuffle(TF.map((_,i)=>i)); i=0; score=0;
    document.getElementById('tfScore').textContent='‡∏Ñ‡∏∞‡πÅ‡∏ô‡∏ô 0';
    renderQ();
  };

  renderQ();
})();
</script>

</body>
</html>
